    {
        "key": "shift+alt+m",
        "command": "workbench.action.toggleMaximizedPanel"
    },
    {
        "key": "alt+i alt+p",
        "command": "extension.pasteImage",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+v",
        "command": "-extension.pasteImage",
        "when": "editorTextFocus"
    }
]
